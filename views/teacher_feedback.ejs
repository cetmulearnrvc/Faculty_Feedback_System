<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Teacher Feedback</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }
        table, th, td {
            border: 1px solid black;
        }
        th, td {
            padding: 10px;
            text-align: left;
        }
    </style>
</head>
<body>
    <h2>Feedback for Teacher</h2>
    <table>
        <tr>
            <th>Student</th>
            <th>Q1</th>
            <th>Q2</th>
            <th>Q3</th>
            <th>Q4</th>
            <th>Q5</th>
            <th>Suggestions</th>
        </tr>
        <% feedbacks.forEach(feedback => { %>
            <tr>
                <td><%= feedback.student_name %></td>
                <td><%= feedback.q1 %></td>
                <td><%= feedback.q2 %></td>
                <td><%= feedback.q3 %></td>
                <td><%= feedback.q4 %></td>
                <td><%= feedback.q5 %></td>
                <td><%= feedback.suggestions %></td>
            </tr>
        <% }); %>
    </table>
</body>
</html>
